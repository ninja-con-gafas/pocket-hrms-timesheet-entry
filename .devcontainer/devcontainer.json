{
  "name": "pocket-hrms-timesheet-entry",
  "build": {
    "dockerfile": "Containerfile"
  },
  "customizations": {
    "root": {
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python",
        "jupyter.jupyterServerType": "local"
      },
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "forwardPorts": [8888],
  "remoteUser": "root"
}
